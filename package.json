{
  "name": "warp-browser-gamepad",
  "version": "1.0.0",
  "description": "Manage your gamepads easily",
  "keywords": [
    "gamepad",
    "gamepads",
    "es6",
    "react",
    "angular",
    "vue",
    "getGamepads"
  ],
  "license": "MIT",
  "homepage":
    "https://github.com/wearereasonablepeople/warp-browser-gamepad#readme",
  "bugs": {
    "url":
      "https://github.com/wearereasonablepeople/warp-browser-gamepad/issues"
  },
  "repository": {
    "type": "git",
    "url":
      "git+https://github.com/wearereasonablepeople/warp-browser-gamepad.git"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/starter.min.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "build": "babel src -d lib && BABEL_ENV=es babel src -d es",
    "precommit": "npm test && lint-staged",
    "prepublish": "rm -rf lib && rm -rf es && npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.4",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.1.5",
    "eslint": "4.19.1",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-config-warp": "^3.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "7.1.1",
    "prettier": "1.12.1",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "style-loader": "0.21.0",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier-eslint --write", "git add"]
  },
  "jest": {
    "testURL": "http://localhost:3000",
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/))\\.test.js?$",
    "moduleFileExtensions": ["js", "json"]
  }
}
